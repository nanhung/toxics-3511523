	author = {Aylward, Lesa L. and Irwin, Kim and St-Amand, Annie and Nong, Andy and Hays, Sean M.},
	date = {2018-02},
	pmid = {29113940},
	keywords = {Pesticides, Risk assessment, Biomarkers, Biomonitoring, Humans, Risk Assessment, Environmental Exposure, Environmental Monitoring, Pyrethrins, Nitriles, Environmental Pollutants, Insecticides, Biomonitoring Equivalents, Pyrethroids, Benzoates},
