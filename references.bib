	month = nov,
	year = {2011},
	keywords = {Bifenthrin, Blood concentrations, Brain concentrations, Motor activity},
	pages = {1--6},
	file = {Full Text:/Users/nan/Zotero/storage/4MK8GS5Y/Scollon et al. - 2011 - Correlation of tissue concentrations of the pyreth.pdf:application/pdf;ScienceDirect Snapshot:/Users/nan/Zotero/storage/WYMWJFTJ/S0300483X11002939.html:text/html},
